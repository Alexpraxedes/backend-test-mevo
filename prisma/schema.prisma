generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model ValidOperation {
  id            Int      @id @default(autoincrement())
  createdAt     DateTime @default(now())

  from          String
  to            String
  amount        Int
  suspected     Boolean
}

model InvalidOperation {
  id                Int      @id @default(autoincrement())
  createdAt         DateTime @default(now())

  file              String
  negativeCount     Int
  duplicateCount    Int
  supectedsCount    Int
}